generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  task Task[]
}

model Task {
  id     Int    @id @default(autoincrement())
  task   String
  isDone Boolean?
  userId Int
  user   User   @relation(fields: [userId], references: [id])
}
